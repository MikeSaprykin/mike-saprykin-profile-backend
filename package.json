{
  "name": "mike-saprykin-profile-backend",
  "version": "0.0.1",
  "description": "A backend app for profile application",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "ts-node app/server.ts",
    "precommit": "lint-staged",
    "prettier": "prettier --parser typescript --single-quote --trailing-comma es5 --write \"./**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeSaprykin/mike-saprykin-profile-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MikeSaprykin/mike-saprykin-profile-backend/issues"
  },
  "lint-staged": {
    "*.ts": [
      "yarn prettier",
      "git add"
    ]
  },
  "homepage": "https://github.com/MikeSaprykin/mike-saprykin-profile-backend#readme",
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "@types/supertest": "^2.0.3",
    "codelyzer": "^3.2.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "supertest": "^3.0.0",
    "ts-jest": "^21.1.3",
    "tslint": "^5.7.0"
  },
  "dependencies": {
    "@types/express": "^4.0.37",
    "@types/node": "^8.0.44",
    "express": "^4.16.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
