{
  "name": "mike-saprykin-profile-backend",
  "version": "0.0.1",
  "description": "A backend app for profile application",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV='testing' jest --coverage",
    "start": "nodemon",
    "server": "ts-node app/index.ts",
    "precommit": "lint-staged",
    "prettier": "prettier --parser typescript --single-quote --trailing-comma es5 --write \"./**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeSaprykin/mike-saprykin-profile-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MikeSaprykin/mike-saprykin-profile-backend/issues"
  },
  "lint-staged": {
    "*.ts": [
      "yarn prettier",
      "git add"
    ]
  },
  "homepage": "https://github.com/MikeSaprykin/mike-saprykin-profile-backend#readme",
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "@types/supertest": "^2.0.3",
    "cross-env": "^5.1.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "lint-staged": "^4.2.3",
    "mockgoose": "^7.3.3",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "supertest": "^3.0.0",
    "ts-jest": "^21.1.3",
    "tslint": "^5.7.0"
  },
  "dependencies": {
    "@types/express": "^4.0.37",
    "@types/express-graphql": "0.0.34",
    "@types/graphql": "^0.11.5",
    "@types/mongoose": "^4.7.23",
    "@types/node": "^8.0.44",
    "@types/swagger-jsdoc": "0.0.1",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "mongoose": "^4.12.3",
    "swagger-jsdoc": "^1.9.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "html",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true
  }
}
